{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-console": "off",
    "no-path-concat": "off",
    "no-multiple-empty-lines": "off",
    "func-names": "off",
    "quotes": "off",
    "prefer-template": "off",
    "prefer-arrow-callback": "error",
    "no-underscore-dangle": 0,
    "consistent-return": "off",
    "no-useless-escape": "off"
  }
}
